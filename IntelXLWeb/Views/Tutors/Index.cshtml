        function validateRange(currentEle) {
            let minParagraph = currentEle.find(".min-value");
            let maxParagraph = currentEle.find(".max-value");
            const rangeFill = currentEle.find(".range-fill");
            const minInput = currentEle.find(".min-price");
            const maxInput = currentEle.find(".max-price");
            let minPrice = parseInt(minInput.val()) || 0;
            let maxPrice = parseInt(maxInput.val()) || 0;

            if (minPrice > maxPrice) {
                // Swap values if needed
                let tempValue = maxPrice;
                maxPrice = minPrice;
                minPrice = tempValue;
            }

            const minValue = 0;
            const maxValue = 500;
            const minPercentage = ((minPrice - minValue) / (maxValue - minValue)) * 100;
            const maxPercentage = ((maxPrice - minValue) / (maxValue - minValue)) * 100;
            rangeFill.css({
                left: minPercentage + "%",
                width: maxPercentage - minPercentage + "%"
            });

            minParagraph.text("₹" + minPrice);
            maxParagraph.text("₹" + maxPrice);
        }
        $(document).ready(function () {
            // Initial call to validateRange
            $(".ix-pricerange").each(function (e) {
                validateRange($(this));
            });

            // Add an event listener to each input element using jQuery
            $(".min-price, .max-price").on("input", function () {
                validateRange($(this).closest(".ix-pricerange"));
            });

        });

