        function getDayOfWeek(day) {
            // Map days to the corresponding day of the week (Sunday is 0, Monday is 1, etc.)
            var dayMap = {
                Sunday: 0,
                Monday: 1,
                Tuesday: 2,
                Wednesday: 3,
                Thursday: 4,
