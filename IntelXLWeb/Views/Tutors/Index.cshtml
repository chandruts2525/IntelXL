@section Styles{
    <style>
        /*.....Calendar styles.....*/
        .fc .fc-cell-shaded, .fc .fc-day-disabled {
            background: #ebe8e84d;
        }

        .fc .fc-highlight {
            background: #98d98e;
        }

        @@media (max-width: 575.98px) {
            .fc-view-harness.fc-view-harness-active {
                height: 630px !important;
            }
        }

        .time-selection {
            min-width: 150px;
            height: 35px;
            display: flex;
            justify-content: center;
            align-items: center;
            box-sizing: border-box;
            background-color: #fff;
            cursor: pointer;
            outline: 0;
        }

        .fc-event-time::after {
            content: "" !important;
        }

        .price-content {
            display: flex;
            align-items: center;
            justify-content: space-between;
            margin: 0 10px;
        }

        label {
            font-size: 14px;
            font-weight: 500;
        }

        p {
            font-size: 16px;
            font-weight: 600;
        }

        .range-slider {
            width: 100%;
            position: relative;
            margin: 2px 0 15px 0;
        }

        .range-fill {
            height: 6px;
            background-color: #36b37e;
            position: absolute;
            z-index: 1;
        }

        input[type="range"] {
            -webkit-appearance: none;
            width: 100%;
            background: transparent;
            position: absolute;
            left: 0;
        }

            input[type="range"]::-webkit-slider-thumb {
                -webkit-appearance: none;
                height: 15px;
                width: 15px;
                border-radius: 50%;
                background: #36b37e;
                cursor: pointer;
                margin-top: -5px;
                position: relative;
                z-index: 1;
            }

            input[type="range"]::-webkit-slider-runnable-track {
                width: 100%;
                height: 5px;
                background: #e8e8e8;
                border-radius: 3px;
                border: none;
            }

        .sidenav {
            height: 100%;
            width: 0;
            position: absolute;
            z-index: 9;
            top: 0;
            right: 0;
            background-color: #fff;
            overflow-x: hidden;
            transition: 0.5s;
        }

            .sidenav .close-sidenav a {
                padding: 8px 8px 8px 32px;
                text-decoration: none;
                color: #818181;
                display: block;
                transition: 0.3s;
            }

        .clear-filter a {
            text-decoration: underline;
        }

        .sidenav.close-sidenav a:hover {
            color: #f1f1f1;
        }

        .sidenav .closebtn {
            font-size: 36px;
        }

        .filter-ico {
            display: none;
        }

        @@media screen and (max-width: 700px) {
            .filter-ico {
                display: block;
            }

            .hidable-content {
                display: none;
            }

            .course-select {
                width: 85%;
            }
        }

        @@media screen and (max-height: 450px) {
            .sidenav {
                padding-top: 15px;
            }

                .sidenav a {
                    font-size: 18px;
                }
        }

        .tutor-option-select, .sidenav-hdr {
            border: none;
            border-radius: 0;
            border-bottom: 1px solid #ddd;
        }

        .sort-item input {
            width: 30px;
            height: 30px;
        }

        .sidenav-item {
            border-bottom: 1px solid #ddd;
        }

        .ix-sort-checkbox:checked {
            background-color: #36b37e;
            border-color: #36b37e;
        }

        .sidenav-item .ix-drp-icon {
            border: none;
            border-radius: 0;
            border-bottom: 1px solid #ddd;
        }

        .filter-btn {
            background-color: #36b37e;
            border: 1px solid #000;
            color: #fff;
            width: 100%;
        }

        .sidenav-item .ix-drp-icon {
            background-image: none;
        }

        .sidenav-item .price-content {
            display: flex;
            align-items: center;
            justify-content: center;
        }

        .time-selection {
            cursor: pointer;
            border: 1px solid #ced4da;
            padding: 10px;
        }
        /* Time selecetion css*/
        .time-selection-left {
            border-radius: 10px 0 0 10px;
            border-right: 0;
        }

        .time-selection-right {
            border-radius: 0 10px 10px 0;
        }

        .time-selection.active {
            background-color: #198754;
            color: #fff;
        }
        .calendar {
            padding: 8px;
            background: #ffffff;
            border-radius: 4px;
            font-size: 18px;
            border: 1px solid #333333;
            box-shadow: 0px 20px 40px rgba(59,60,56,0.05);
        }

        .ui-datepicker {
            background: #ffffff;
            border-radius: 15px;
        }

        .ui-datepicker-header {
            height: 50px;
            line-height: 50px;
            color: #ffffff;
            background: #198754;
            margin-bottom: 10px;
        }

        .ui-datepicker-prev,
        .ui-datepicker-next {
            width: 20px;
            height: 20px;
            text-indent: 9999px;
            border-radius: 100%;
            cursor: pointer;
            overflow: hidden;
            margin-top: 12px;
        }

        .ui-datepicker-prev {
            float: left;
            margin-left: 12px;
        }

            .ui-datepicker-prev:after {
                transform: rotate(45deg);
                margin: -43px 0px 0px 8px;
            }

        .ui-datepicker-next {
            float: right;
            margin-right: 12px;
        }

            .ui-datepicker-next:after {
                transform: rotate(-135deg);
                margin: -43px 0px 0px 6px;
            }

            .ui-datepicker-prev:after,
            .ui-datepicker-next:after {
                content: '';
                position: absolute;
                display: block;
                width: 8px;
                height: 8px;
                border-left: 2px solid #ffffff;
                border-bottom: 2px solid #ffffff;
            }

            .ui-datepicker-prev:hover,
            .ui-datepicker-next:hover,
            .ui-datepicker-prev:hover:after,
            .ui-datepicker-next:hover:after {
                border-color: #333333;
            }

        .ui-datepicker-title {
            text-align: center;
            font-size: 25px;
        }

        .ui-datepicker-calendar {
            width: 100%;
            text-align: center;
        }

            .ui-datepicker-calendar thead tr th span {
                display: block;
                width: 40px;
                color: #198754;
                margin-bottom: 5px;
                font-size: 18px;
            }

        .ui-state-default {
            display: block;
            text-decoration: none;
            color: #333333;
            line-height: 40px;
            font-size: 16px;
        }

        .ui-state-highlight {
            color: #ffffff;
            background-color: #198754;
            border-radius: 50px;
        }

        .ui-state-active {
            color: #ffffff;
            background-color: #198754;
            border-radius: 50px;
        }

        .ui-datepicker-unselectable .ui-state-default {
            color: #b9b6b6;
            /* border: 2px solid transparent;*/
        }

        .ui-state-default:hover {
            color: #ffffff;
            background: #198754;
            border-radius: 50px;
            transition: all 0.25s cubic-bezier(0.7, -0.12, 0.2, 1.12);
        }

        .ix-tutor-anime {
            border: 1px solid #c5c5c5;
            border-radius: 5px;
        }
    </style>
}
